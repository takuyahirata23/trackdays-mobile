{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["./*"],
      "@components/*": ["components/*"],
      "@components": ["components"],
      "@context/*": ["context/*"],
      "@graphql/*": ["graphql/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"]
    },
    "strict": true,
    "moduleResolution": "node"
  }
}
