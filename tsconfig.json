{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@context/*": [
        "context/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@graphql/*": [
        "graphql/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@rest/*": [
        "rest/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@type/*": [
        "type/*"
      ],
      "@functions/*": [
        "functions/*"
      ]
    },
    "strict": true
  },
  "include": [
    "./jest-setup.js",
    "**/*.ts",
    "**/*.tsx"
  ]
}
