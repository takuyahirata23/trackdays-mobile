{
  "expo": {
    "name": "Motorcycle Trackdays",
    "slug": "motorcycle-trackdays",
    "version": "2.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "usesNonExemptEncryption": false,
      "bundleIdentifier": "com.takuya23.trackdaysmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ],
      "package": "com.takuya23.trackdaysmobile",
      "versionCode": 12
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1bf06458-8f75-43ba-a21a-dbf6f4b66c83"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar if you would like the app to add trackdays to your calendar."
        }
      ]
    ],
    "owner": "takuya23"
  }
}
